{
  "name": "litepad",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "http://localhost:5173",
  "node-remote": "http://localhost:5173",
  "window": {
    "id": "litepad-gui",
    "title": "LitePad",
    "position": "center",
    "height": 660,
    "width": 1060,
    "min_height": 600,
    "min_width": 800,
    "icon": "public/appicon.png"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:nw\"",
    "dev:app": "vite",
    "dev:nw": "nwbuild --mode=run --glob=false --flavor=sdk",
    "build": "concurrently \"npm run build:app\" \"npm run build:nw\"",
    "build:app": "run-p type-check \"build-only {@}\" --",
    "build:nw": "node ./build.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@primevue/themes": "^4.0.1",
    "dexie": "^4.0.8",
    "md5": "^2.3.0",
    "nanoid": "^5.0.7",
    "performant-array-to-tree": "^1.11.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.1",
    "vue": "3.4.33",
    "vue-router": "^4.3.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.14.5",
    "@types/nw.js": "^0.13.21",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "base-volta-off-of-nwjs": "^1.0.5",
    "browserslist": "^4.23.2",
    "concurrently": "^8.2.2",
    "jsdom": "^24.1.0",
    "lightningcss": "^1.25.1",
    "npm-run-all2": "^6.2.0",
    "nw": "^0.89.0",
    "nw-builder": "^4.8.0",
    "typescript": "~5.4.0",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21",
    "wait-on": "^7.2.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vue/runtime-core@3.4.33": "patches/@vue__runtime-core@3.4.33.patch"
    }
  }
}
